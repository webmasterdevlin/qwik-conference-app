{
  "name": "my-qwik-basic-starter",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "homepage": "https://qwik.builder.io/",
  "license": "",
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adaptors/express/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node --inspect-brk server/entry.express",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "test:unit": "vitest",
    "test:unit.watch": "vitest watch",
    "test:unit.converage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e.debug": "playwright test --debug",
    "test:e2e.codegen": "npx playwright codegen http://localhost:5173"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@builder.io/qwik": "0.104.0",
    "@builder.io/qwik-city": "0.104.0",
    "@builder.io/qwik-react": "0.5.0",
    "@emotion/react": "11.10.6",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.6",
    "@mui/material": "5.12.2",
    "@mui/system": "^5.12.1",
    "@mui/x-data-grid": "6.2.1",
    "@playwright/test": "^1.33.0",
    "@react-three/fiber": "^8.12.2",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/compression": "^1.7.2",
    "@types/eslint": "8.37.0",
    "@types/express": "4.17.17",
    "@types/node": "latest",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "@vitest/coverage-c8": "^0.30.1",
    "@vitest/ui": "^0.30.1",
    "autoprefixer": "^10.4.14",
    "axios": "^1.3.6",
    "compression": "^1.7.4",
    "eslint": "8.39.0",
    "eslint-plugin-qwik": "0.104.0",
    "express": "4.18.2",
    "jsdom": "^21.1.1",
    "node-fetch": "3.3.1",
    "postcss": "^8.4.23",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.3.2",
    "three": "^0.151.3",
    "typescript": "5.0.4",
    "undici": "^5.22.0",
    "vite": "4.3.3",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "^0.30.1",
    "zod": "^3.21.4"
  }
}
