{
  "name": "my-qwik-basic-starter",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "homepage": "https://qwik.builder.io/",
  "license": "",
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adaptors/express/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node --inspect-brk server/entry.express",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "test:unit": "vitest",
    "test:unit.watch": "vitest watch",
    "test:unit.converage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e.debug": "playwright test --debug",
    "test:e2e.codegen": "npx playwright codegen http://localhost:5173"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@builder.io/qwik": "1.2.6",
    "@builder.io/qwik-city": "1.2.6",
    "@builder.io/qwik-react": "0.5.0",
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@mui/material": "5.14.0",
    "@mui/system": "^5.14.0",
    "@mui/x-data-grid": "6.10.0",
    "@playwright/test": "^1.36.1",
    "@react-three/fiber": "^8.13.5",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/compression": "^1.7.2",
    "@types/eslint": "8.44.0",
    "@types/express": "4.17.17",
    "@types/node": "latest",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@types/testing-library__jest-dom": "^5.14.8",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "compression": "^1.7.4",
    "eslint": "8.45.0",
    "eslint-plugin-qwik": "1.2.6",
    "express": "4.18.2",
    "jsdom": "^22.1.0",
    "node-fetch": "3.3.1",
    "postcss": "^8.4.26",
    "prettier": "3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.3.3",
    "three": "^0.154.0",
    "typescript": "5.1.6",
    "undici": "^5.22.1",
    "vite": "4.4.4",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "^0.33.0",
    "zod": "^3.21.4"
  }
}
